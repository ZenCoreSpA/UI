{
    "name": "zencorespa/ui",
    "description": "Package to provide UI",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ZenCoreSpA\\UI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZenCoreSpA\\UI\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Ian Torres",
            "email": "iantorres@outlook.com"
        }
